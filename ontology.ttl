@prefix : <http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl: <http://www.w3.org/2003/11/swrl#> .
@prefix swrlb: <http://www.w3.org/2003/11/swrlb#> .
@prefix terms: <http://purl.org/dc/terms/> .
@base <http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/> .

<http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys> rdf:type owl:Ontology ;
                                                                                       owl:versionIRI <http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/1.0.0> ;
                                                                                       terms:abstract "This ontology is designed to represent and organize information derived from airline passenger satisfaction surveys, with an initial focus on Qatar Airways and Ryanair. The ontology will allow the capture, consultation and analysis of data on the passenger experience."@en ;
                                                                                       terms:license "https://creativecommons.org/licenses/by-sa/4.0/"@en ;
                                                                                       terms:title "Flight Satisfaction Surveys Ontology"@en ;
                                                                                       rdfs:comment "Ontology of Flight Satisfaction Surveys, by Alejandro Roldán Soblechero"@en ;
                                                                                       rdfs:label "Flight Satisfaction Surveys Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/abstract
terms:abstract rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/arrivesAtAirport
:arrivesAtAirport rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain :Flight ;
                  rdfs:range :Airport ;
                  rdfs:comment "The airport at which the flight arrives."@en ;
                  rdfs:label "arrives at airport"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/departsFromAirport
:departsFromAirport rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Flight ;
                    rdfs:range :Airport ;
                    rdfs:comment "The airport from which the flight departs."@en ;
                    rdfs:label "departs from airport"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/hasAircraft
:hasAircraft rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Flight ;
             rdfs:range :Aircraft ;
             rdfs:comment "Associates a flight with a specific aircraft used for that flight."@en ;
             rdfs:label "has aircraft"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/hasAssignedSeat
:hasAssignedSeat rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :Passenger ;
                 rdfs:range :Seat ;
                 rdfs:comment "Connects a passenger to a specific seat they are assigned on a flight."@en ;
                 rdfs:label "has assigned seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/hasSeat
:hasSeat rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Flight ;
         rdfs:range :Seat ;
         rdfs:comment "Links a flight to the specific seat occupied during the flight."@en ;
         rdfs:label "has seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/isPartOfCountry
:isPartOfCountry rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :City ;
                 rdfs:range :Country ;
                 rdfs:comment "Indicates the country in which the city is located."@en ;
                 rdfs:label "is part of country"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/livesInCountry
:livesInCountry rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Passenger ;
                rdfs:range :Country ;
                rdfs:comment "Indicates the country where the passenger resides."@en ;
                rdfs:label "lives in country"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/locatedInCity
:locatedInCity rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Airport ;
               rdfs:range :City ;
               rdfs:comment "Indicates the city where the airport is located."@en ;
               rdfs:label "located in city"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/operatedByAirline
:operatedByAirline rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :Flight ;
                   rdfs:range :Airline ;
                   rdfs:comment "Indicates the airline that operates a specific flight."@en ;
                   rdfs:label "operated by airline"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/providesService
:providesService rdf:type owl:ObjectProperty ;
                 rdfs:domain :Airline ;
                 rdfs:range :Service ;
                 rdfs:comment "Associates an airline with the services it provides, such as cabin or ground services."@en ;
                 rdfs:label "provides service"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/receivesService
:receivesService rdf:type owl:ObjectProperty ;
                 rdfs:domain :Seat ;
                 rdfs:range :Service ;
                 rdfs:comment "Links a seat to specific services provided during a flight, reflecting the level and type of service based on the seat’s location and class." ;
                 rdfs:label "receives service"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/reviewOfFlight
:reviewOfFlight rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :FlightReview ;
                rdfs:range :Flight ;
                rdfs:comment "Associates a review with the specific flight it addresses."@en ;
                rdfs:label "review of flight"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/reviewedByPassenger
:reviewedByPassenger rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :FlightReview ;
                     rdfs:range :Passenger ;
                     rdfs:comment "Connects a flight review to the passenger who wrote the review."@en ;
                     rdfs:label "reviewed by passenger"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/worksOnFlight
:worksOnFlight rdf:type owl:ObjectProperty ;
               rdfs:domain :CrewMember ;
               rdfs:range :Flight ;
               rdfs:comment "Connects a crew member to the flights they work on."@en ;
               rdfs:label "works on flight"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/comment
:comment rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :FlightReview ;
         rdfs:range xsd:string ;
         rdfs:comment "The detailed text content of the flight review." ;
         rdfs:label "comment"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/date
:date rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Flight ;
      rdfs:range xsd:dateTime ;
      rdfs:comment "The date on which the flight takes place."@en ;
      rdfs:label "date"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/model
:model rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :Aircraft ;
       rdfs:range xsd:string ;
       rdfs:comment "The model of the aircraft used for a flight."@en ;
       rdfs:label "model"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/name
:name rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Place ;
      rdfs:range xsd:string ;
      rdfs:comment "The name of the geographical location, such as a city, country, or airport."@en ;
      rdfs:label "name"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/number
:number rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :Flight ;
        rdfs:range xsd:string ;
        rdfs:comment "The unique identifier for each flight."@en ;
        rdfs:label "number"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/rating
:rating rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :FlightReview ;
        rdfs:range xsd:integer ;
        rdfs:comment "The rating given by a passenger in a flight review."@en ;
        rdfs:label "rating"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/recommendationStatus
:recommendationStatus rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :FlightReview ;
                      rdfs:range xsd:boolean ;
                      rdfs:comment "Indicates whether the passenger recommends the flight or not, represented as a boolean value."@en ;
                      rdfs:label "recommendation status"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/title
:title rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain :FlightReview ;
       rdfs:range xsd:string ;
       rdfs:comment "The title of the flight review, providing a brief summary or headline."@en ;
       rdfs:label "title"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/verified
:verified rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :FlightReview ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Indicates whether the flight review has been verified as authentic by the airline or a third-party platform."@en ;
          rdfs:label "verified"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:comment "A vehicle designed for air transport"@en ;
          rdfs:label "Aircraft"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Airline
:Airline rdf:type owl:Class ;
         rdfs:comment "A company that provides air transport services"@en ;
         rdfs:label "Airline"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Airport
:Airport rdf:type owl:Class ;
         rdfs:subClassOf :Place ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :locatedInCity ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :City
                         ] ;
         rdfs:comment "A facility used for the arrival, departure, and maintenance of aircraft, equipped with runways and passenger facilities."@en ;
         rdfs:label "Airport"@en ;
         rdfs:sameAs "https://dbpedia.org/page/Airport"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/BusinessClassSeat
:BusinessClassSeat rdf:type owl:Class ;
                   rdfs:subClassOf :Seat ;
                   rdfs:comment "A high-comfort seat, typically found in the business class section of an aircraft."@en ;
                   rdfs:label "Business Class Seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/BusinessTraveller
:BusinessTraveller rdf:type owl:Class ;
                   rdfs:subClassOf :Passenger ;
                   rdfs:comment "A passenger who travels primarily for business purposes."@en ;
                   rdfs:label "Business Traveller"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/CabinService
:CabinService rdf:type owl:Class ;
              rdfs:subClassOf :Service ;
              rdfs:comment "Services offered inside the aircraft cabin, such as meals, drinks, and in-flight entertainment."@en ;
              rdfs:label "Cabin Service"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/City
:City rdf:type owl:Class ;
      rdfs:subClassOf :Place ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :isPartOfCountry ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Country
                      ] ;
      rdfs:comment "An urban area that serves as a hub."@en ;
      rdfs:label "City"@en ;
      rdfs:sameAs "https://dbpedia.org/page/City"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Country
:Country rdf:type owl:Class ;
         rdfs:subClassOf :Place ;
         rdfs:comment "A distinct territorial body or political entity known for its sovereignty and governance."@en ;
         rdfs:label "Country"@en ;
         rdfs:sameAs "https://dbpedia.org/page/Country"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/CrewMember
:CrewMember rdf:type owl:Class ;
            rdfs:comment "An individual who is part of the flight staff"@en ;
            rdfs:label "Crew Member"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/DomesticFlight
:DomesticFlight rdf:type owl:Class ;
                rdfs:subClassOf :Flight ;
                rdfs:comment "A flight that takes place within the boundaries of a single country."@en ;
                rdfs:label "Domestic Flight"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/EconomySeat
:EconomySeat rdf:type owl:Class ;
             rdfs:subClassOf :Seat ;
             rdfs:comment "A standard seat within the main cabin area, optimized for density and affordability."@en ;
             rdfs:label "Economy Seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/FirstClassSeat
:FirstClassSeat rdf:type owl:Class ;
                rdfs:subClassOf :Seat ;
                rdfs:comment "The most spacious seat available on an aircraft, offering premium services."@en ;
                rdfs:label "First Class Seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Flight
:Flight rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSeat ;
                          owl:allValuesFrom :Seat
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :arrivesAtAirport ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Airport
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :departsFromAirport ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Airport
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAircraft ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Aircraft
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :operatedByAirline ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Airline
                        ] ;
        rdfs:comment "A scheduled journey of an aircraft from one airport to another."@en ;
        rdfs:label "Flight"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/FlightAttendant
:FlightAttendant rdf:type owl:Class ;
                 rdfs:subClassOf :CrewMember ;
                 rdfs:comment "A crew member who ensures the safety and comfort of passengers aboard a flight."@en ;
                 rdfs:label "Flight Attendant"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/FlightReview
:FlightReview rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :reviewOfFlight ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Flight
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :reviewedByPassenger ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Passenger
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :rating ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:onDatatype xsd:integer ;
                                                     owl:withRestrictions ( [ xsd:maxInclusive 10
                                                                            ]
                                                                          )
                                                   ]
                              ] ;
              rdfs:comment "A  evaluation or feedback provided by a passenger regarding their experience on a flight."@en ;
              rdfs:label "Flight Review"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/GroundService
:GroundService rdf:type owl:Class ;
               rdfs:subClassOf :Service ;
               rdfs:comment "Services provided at the airport before departure or after arrival, including check-in and baggage handling."@en ;
               rdfs:label "Ground Service"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/InternationalFlight
:InternationalFlight rdf:type owl:Class ;
                     rdfs:subClassOf :Flight ;
                     rdfs:comment "A flight that crosses international boundaries between two or more countries."@en ;
                     rdfs:label "International Flight"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/LeisureTraveller
:LeisureTraveller rdf:type owl:Class ;
                  rdfs:subClassOf :Passenger ;
                  rdfs:comment "A passenger who travels primarily for leisure or vacation purposes."@en ;
                  rdfs:label "Leisure Traveller"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Passenger
:Passenger rdf:type owl:Class ;
           rdfs:comment "An individual who travels in an aircraft, potentially with different travel purposes and preferences."@en ;
           rdfs:label "Passenger"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Pilot
:Pilot rdf:type owl:Class ;
       rdfs:subClassOf :CrewMember ;
       rdfs:comment "A crew member trained and certified to operate an aircraft."@en ;
       rdfs:label "Pilot"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Place
:Place rdf:type owl:Class ;
       rdfs:comment "A location that can be geographically identified."@en ;
       rdfs:label "Place"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Seat
:Seat rdf:type owl:Class ;
      rdfs:comment "A specific place for a passenger to sit in an aircraft."@en ;
      rdfs:label "Seat"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Service
:Service rdf:type owl:Class ;
         rdfs:comment "Services provided by the airline to enhance passenger experience."@en ;
         rdfs:label "Service"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Airbus_A380
:Airbus_A380 rdf:type owl:NamedIndividual ,
                      :Aircraft ;
             rdfs:comment "Aircraft model of Airbus A380"@en ;
             rdfs:label "Airbus A380"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Ali_Al-Hamed
:Ali_Al-Hamed rdf:type owl:NamedIndividual ,
                       :Passenger ;
              :hasAssignedSeat :Business_Class_Seat_1A_on_Flight_DOH-LPL ;
              :livesInCountry :Qatar ;
              rdfs:comment "Ali Al-Hamed Passenger"@en ;
              rdfs:label "Ali Al-Hamed"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Boeing_787
:Boeing_787 rdf:type owl:NamedIndividual ,
                     :Aircraft ;
            rdfs:comment "Aircraft model of Boeing 787"@en ;
            rdfs:label "Boeing 787"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Business_Class_Seat_1A_on_Flight_DOH-LPL
:Business_Class_Seat_1A_on_Flight_DOH-LPL rdf:type owl:NamedIndividual ,
                                                   :BusinessClassSeat ;
                                          rdfs:comment "Business Class Seat 1A on Flight DOH-LPL"@en ;
                                          rdfs:label "Business Class Seat 1A on Flight DOH-LPL"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Doha
:Doha rdf:type owl:NamedIndividual ,
               :City ;
      :isPartOfCountry :Qatar ;
      rdfs:comment "City Doha"@en ;
      rdfs:label "Doha"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Doha_Airport
:Doha_Airport rdf:type owl:NamedIndividual ,
                       :Airport ;
              :locatedInCity :Doha ;
              rdfs:comment "Doha Airport"@en ;
              rdfs:label "Doha Airport"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Economy_Class_Seat_23C_on_Flight_EDI-LPL
:Economy_Class_Seat_23C_on_Flight_EDI-LPL rdf:type owl:NamedIndividual ,
                                                   :EconomySeat ;
                                          rdfs:comment "Economy Class Seat 23C on Flight EDI-LPL"@en ;
                                          rdfs:label "Economy Class Seat 23C on Flight EDI-LPL"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Edinburgh
:Edinburgh rdf:type owl:NamedIndividual ,
                    :City ;
           :isPartOfCountry :United_Kingdom ;
           rdfs:comment "City Edinburgh"@en ;
           rdfs:label "Edinburgh"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Edinburgh_Airport
:Edinburgh_Airport rdf:type owl:NamedIndividual ,
                            :Airport ;
                   :locatedInCity :Edinburgh ;
                   rdfs:comment "Edinburgh Airport"@en ;
                   rdfs:label "Edinburgh Airport"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Flight_DOH-LPL_01
:Flight_DOH-LPL_01 rdf:type owl:NamedIndividual ,
                            :Flight ;
                   :arrivesAtAirport :Liverpool_John_Lennon_Airport ;
                   :departsFromAirport :Doha_Airport ;
                   :hasAircraft :Airbus_A380 ;
                   :hasSeat :Business_Class_Seat_1A_on_Flight_DOH-LPL ;
                   :operatedByAirline :Qatar_Airways ;
                   rdfs:comment "Flight 01 from Doha to Liverpool"@en ;
                   rdfs:label "Flight DOH-LPL 01"@en .


###  http://www.semanticweb.org/soblechero/ontologies/2024/3/flight-satisfaction-surveys/Flight_EDI-LPL_01
:Flight_EDI-LPL_01 rdf:type owl:NamedIndividual ,
                            :Flight ;
                   :arrivesAtAirport :Liverpool_John_Lennon_Airport ;
                   :departsFromAirport :Edinburgh_Airport ;
                   :hasAircraft :Boeing_787 ;
                   :hasSeat :Economy_Class_Seat_23C_on_Flight_EDI-LPL ;
                   :operatedByAirline :Ryanair ;
